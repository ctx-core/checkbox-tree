{
	"name": "@ctx-core/checkbox-tree",
	"version": "15.0.136",
	"description": "ctx-core checkbox tree component support library",
	"keywords": [
		"ctx-core",
		"Checkbox_Tree",
		"svelte",
		"component"
	],
	"homepage": "https://github.com/ctx-core/checkbox-tree#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/checkbox-tree/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/checkbox-tree.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-checkbox-tree@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"_types",
		"all__checkbox__onchange",
		"checkbox__onchange"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^7.2.18"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.1",
		"@sveltejs/kit": "^2.5.10",
		"c8": "^10.1.2",
		"check-dts": "^0.8.0",
		"tsx": "^4.15.3",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false
}