{
	"name": "@ctx-core/checkbox-tree",
	"version": "14.4.4",
	"description": "ctx-core checkbox tree component support library",
	"keywords": [
		"ctx-core",
		"Checkbox_Tree",
		"svelte",
		"component"
	],
	"homepage": "https://github.com/ctx-core/checkbox-tree#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/checkbox-tree/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/checkbox-tree.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./all__checkbox__onchange_",
		"./checkbox__onchange_"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "^27.0.4",
		"@ctx-core/nanostores": "^7.1.5",
		"@ctx-core/object": "^30.1.3"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tslib": "^2.6.2",
		"tsx": "^4.1.2",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
}