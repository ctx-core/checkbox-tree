{
	"name": "@ctx-core/checkbox-tree",
	"version": "14.0.13",
	"description": "ctx-core checkbox tree component support library",
	"keywords": [
		"ctx-core",
		"Checkbox_Tree",
		"svelte",
		"component"
	],
	"homepage": "https://github.com/ctx-core/checkbox-tree#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/checkbox-tree/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/checkbox-tree.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.d.ts",
	"svelte": "./src/index.js",
	"exports": {
		".": {
			"import": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build && npm run test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsm node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^24.0.0",
		"@ctx-core/nanostores": "workspace:^0.5.1",
		"@ctx-core/object": "workspace:^22.0.13"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"c8": "^7.11.0",
		"check-dts": "^0.6.6",
		"tslib": "^2.3.1",
		"tsm": "^2.2.1",
		"typescript": "next",
		"uvu": "^0.5.3"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false
